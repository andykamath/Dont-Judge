{
"name":"Don't Judge",
"description":"Help others overcome anxiety induced by social media",
"version":"1",
"manifest_version":2,
"content_scripts": [
    {
      "matches": ["*://*.facebook.com/*","*://*.twitter.com/*","*://*.t.co/*", "*://*.instagram.com/*", "*://*.reddit.com/*"],
      "js": ["index.js"]
    }
  ]
}